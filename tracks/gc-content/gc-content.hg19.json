{
  "title": "GC content",
  "description": "Windowed GC content computed from the reference genome. Extracted with GATK AnnotateIntervals",
  "styles": { "height": 40 },
  "data": { "url": "gc-intervals.hg19.tsv" },
  "mark": {
    "type": "rect",
    "minWidth": 0.5,
    "minOpacity": 1
  },
  "encoding": {
    "x": {
      "chrom": "CONTIG",
      "pos": "START",
      "offset": -1,
      "type": "quantitative"
    },
    "x2": {
      "chrom": "CONTIG",
      "pos": "END"
    },
    "y": {
      "field": "GC_CONTENT",
      "type": "quantitative",
      "scale": {
        "domain": [0.3, 0.7]
      },
      "title": "GC cont."
    }
  }
}
